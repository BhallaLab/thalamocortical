objectvar stim1, outfile, vec_v, vec_t, vec_g
FARADAY=96485
PI=3.141592
secondorder=2
dt=1e-3
celsius=30
create soma
access soma
v_init = -65
soma{
    nseg = 1
    diam = 2 * 2.0 / 3.0
    L = 40
    insert naf2
    gbar_naf2 =   0.15 * 2
    insert napf_spinstell
    gbar_napf_spinstell =   0.00015 * 2
    insert kdr_fs 
    gbar_kdr_fs =   0.1 * 2
    insert kc_fast
    gbar_kc_fast =   0.01 * 2
    insert ka
    gbar_ka =   0.03 * 2
    insert km
    gbar_km =   0.00375 * 2
    insert k2
    gbar_k2 =   0.0001 * 2
    insert kahp_slower
    gbar_kahp_slower =   0.0001 * 2
    insert cal
    gbar_cal =   0.0005 * 2
    insert cat_a
    gbar_cat_a =   0.0001 * 2
    insert ar
    gbar_ar =   0.00025 * 2
    insert cad
    // *** ca diffusion: beta=1/tau
    beta_cad  =   0.02
    // cafor(I) (FORTRAN) converted to phi (NEURON)
    phi_cad =   260000.
    insert pas
    e_pas = -65 
    g_pas = 2e-5 * 2
    Ra = 100.0
    cm = 0.9 * 2
    ek = -100.0
    ena = 50.0
    vca = 125.0
    erev_ar = -40.0
    
    fastNa_shift_naf2=-2.5
    fastNa_shift_napf_spinstell=-2.5
}

stim1 = new IClamp(0.5)
stim1.del = 20.0
stim1.amp = 0.3
stim1.dur = 50
ceiling_cad = 1e6

outfile = new File()
vec_g = new Vector()
vec_v = new Vector()
vec_t = new Vector()
vec_g.record(&soma.gk_naf2(0.5))
vec_v.record(&soma.v(0.5))
vec_t.record(&t)

tstop = 50
run()
outfile.wopen("mydata/Vm.plot")
for ii = 0, vec_v.size() - 1 {
    outfile.printf("%g %g %g\n", vec_t.x(ii), vec_v.x(ii), vec_g.x(ii))
}
outfile.close()
quit()
