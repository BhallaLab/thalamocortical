external traub_connect

objref this, all

create comp[59 + 1]

objref level[9 + 1], soma, dendrites, soma_dendrites, axon

proc init(){
    traub_connect( 59+1)
    comp[0] delete_section()
    presyn_comp = 57
    objref soma, axon, dendrites, soma_dendrites
    objref level
    topol()
    shape()
    geom()
    subsets()
    biophys()
    forsec all if (ismembrane("ar")){ m0_ar = 0 }
    set_doubler()
    if (doubler){ double_dend_cond()}
    more_adjustments()
}

proc set_doubler(){
    doubler = 1
}

